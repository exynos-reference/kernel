config DRM_PTN3460
	tristate "PTN3460 DP/LVDS bridge"
	depends on DRM
	select DRM_KMS_HELPER
	---help---

config DRM_PANEL_BINDER
	tristate "bridge panel binder"
	depends on DRM
	select DRM_KMS_HELPER
	select DRM_PANEL
	---help---

config DRM_PS8622
	tristate "Parade eDP/LVDS bridge"
	depends on DRM
	select DRM_KMS_HELPER
	select BACKLIGHT_LCD_SUPPORT
	select BACKLIGHT_CLASS_DEVICE
	---help---
